DOCUMENT_ROOT=./
VHOSTS_DIR=./docker-compose/config/vhosts
APACHE_LOG_DIR=./docker-compose/logs/apache2
PHP_INI=./docker-compose/config/php/php.ini
MYSQL_DATA_DIR=./docker-compose/data/mysql
MYSQL_LOG_DIR=./docker-compose/logs/mysql

# If you already has the port 80 in use, you can change it (for example if you have Apache)
HOST_MACHINE_UNSECURE_HOST_PORT=80
HOST_MACHINE_SECURE_HOST_PORT=443

# If you already has the port 3306 in use, you can change it (for example if you have MySQL)
HOST_MACHINE_MYSQL_PORT=3306

# If you already has the port 6379 in use, you can change it (for example if you have Redis)
HOST_MACHINE_REDIS_PORT=6379


# MySQL root user password
MYSQL_ROOT_PASSWORD=toor

# Database settings: Username, password and database name
MYSQL_USER=kuapp
MYSQL_PASSWORD=kuapp
MYSQL_DATABASE=kuapp
